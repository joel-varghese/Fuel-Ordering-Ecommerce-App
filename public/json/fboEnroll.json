                            "href": "#",
                            "name": "okButton",
                            "text": "OK",
                            "target": "_self"
                        }
                    },
                    "validateModal": {
                        "title": "",
                        "paragraph": "Please verify. A required field is missing information",
                        "primaryButton": {
                            "id": "17682",
                            "href": "#",
                            "name": "okButton",
                            "text": "OK",
                            "target": "_self"
                        }
                    }
                }
            ],
            "emailBody":{
                "title": "FBO Enrollment Verification Email",
                "paragraph": "Hello",
                "html":"You're Almost Doneâ€¦ A verification email was sent to: . Open this email and click the link to complete the process."
            },
            "notifyMessage":{
                "msg1": "Let's go! New FBO enrollment request received "
            }
        }
}