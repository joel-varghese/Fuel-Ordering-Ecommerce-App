{
    "fboEnrollData" :
        {
            "headline":{
                "type":"text",
                "label":"FBO Enrollment"
            },
            "sections":[{
                "subSections":[
                    {
                        "fields" :[
                            {
                                "component": "input",
                                "type":"text",
                                "name":"companyName",
                                "maxLength":"100",
                                "label":"Company Name",
                                "placeholder": "Company Name",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6 bf-mrgb-1rem",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    },
                                    {
                                       "type":"onlyspecial",
                                       "validateRule":"[a-zA-Z0-9]+",
                                       "errorMessage":"Invalid Data"
                                    },
                                    {
                                       "type":"onlydigit",
                                       "validateRule":"[a-zA-Z\\.?!#@_=$:]+",
                                       "errorMessage":"Invalid Data"
                                    }
                                ]
                            },
                            {
                                "component": "select",
                                "type":"select",
                                "dynamicSelect": true,
                                "lookupReference": {
                                    "name": "modeOfCommunication"
                                },
                                "name":"preferred_mode_communication",
                                "label":"Preferred Mode of Communication",
                                "placeholder": "Email Or Phone",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6 bf-mrgt2px",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    }
                                ],

                                "options":[
                                    {
                                        "value":"email",
                                        "title":"E-mail"
                                    },
                                    {
                                        "value":"phone",
                                        "title":"Phone"
                                    },
                                    {
                                        "value":"email or phone",
                                        "title":"E-mail or Phone"
                                    }
                                ]
                            },
                            {
                                "component": "input",
                                "type":"text",
                                "name":"firstName",
                                "maxLength":"35",
                                "label":"First Name",
                                "placeholder": "First Name",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    }
                                ]
                            },
                            {
                                "component": "input",
                                "type":"text",
                                "name":"lastName",
                                "maxLength":"35",
                                "label":"Last Name",
                                "placeholder": "Last Name",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    }
                                ]
                            },
                            {
                                "component": "input",
                                "type":"text",
                                "name":"emailId",
                                "maxLength":"50",
                                "label":"Email Address",
                                "placeholder": "Email Address",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    },
                                    {
                                        "type":"IsEmail",
                                        "validateRule":"([A-Za-z0-9_\\-\\.])+\\@([A-Za-z0-9_\\-\\.])+\\.([A-Za-z]{2,4})$",
                                        "errorMessage":"Oops! Invalid E-mail"
                                    }
                                ]
                            },
                            {
                                "component": "input",
                                "type":"text",
                                "name":"contact_number",
                                "minlength":10,
                                "maxLength":"14",
                                "label":"Phone Number",
                                "placeholder": "Phone Number",
                                "isRequired": true,
                                "defaultValue":"",
                                "styles": {
                                    "colWidth": "6",
                                    "className": ""
                                },
                                "validations":[
                                    {
                                        "type":"IsMandatory",
                                        "errorMessage":""
                                    },
                                    {
                                        "type": "CheckUSPhone",
                                        "errorMessage": "Enter a valid Mobile Number"
                                    }
                                ]
                            }
                        ],
                        "primaryButton":[{
                                "component": "button",
                                "type":"button",
                                "label":"Submit",
                                "variant":"primary",
                                "isRequired": true,
                                "styles": {
                                    "colWidth": "",
                                    "className": "bf-btn-login"
                                }
                        }]
                    }
                ]
            }],
            "modal": [
                {
                    "submitModal": {
                        "title": "",
                        "paragraph": "Success! We'll be in touch soon!",
                        "primaryButton": {
                            "id": "17678",
